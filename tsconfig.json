{
    "compilerOptions": {
      "outDir": "./public/build/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "lib": [
        "ES2015",
        "ES2016",
        "ES2017",
        "ES2018",
        "ES2019",
        "ES2020",
        "ESNext", 
        "dom"
      ],
      "esModuleInterop": true
    },
    "include": [
    "./assets/" 
    ],
    "jsx": "react"
  }
